[DEFAULT]
user = %USER%
bind_port = 6000
swift_dir = %TEST_DIR%/etc
devices = %TEST_DIR%
mount_check = false
workers = 1
log_level = DEBUG

[pipeline:main]
pipeline = object-server

[app:object-server]
use = egg:swift#object
allowed_headers = Content-Disposition, Content-Encoding, Content-Language, X-Delete-At, X-Object-Manifest, X-Static-Large-Object, Expires, Cache-Control, X-Robots-Tag
